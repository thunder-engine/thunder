<module name="PartcileUpdate/SizeScale" stage="ParticleUpdate">
    <params>
        <param name="sizeScale" type="vec3" default="0, 0, 0" mode="sizeScaleMode"/>
    </params>
    <operations>
        <operation code="mul" result="vec3" arg0="sizeScale" arg1="s.deltaTime"/>
        <operation code="add" result="p.size" arg0="p.size" arg1="vec3"/>
    </operations>
</module>
