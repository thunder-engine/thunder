<function name="Shapes/Ellipse">
    <inputs>
        <input name="UV" type="vec2" embedded="_uv0"/>
		<input name="Width" type="float" default="0.5"/>
		<input name="Height" type="float" default="0.5"/>
    </inputs>
    <outputs>
        <output name="" type="float"/>
    </outputs>
    <code>
<![CDATA[
float Ellipse(vec2 uv, float width, float height) {
	float d = length((uv * 2.0 - 1.0) / vec2(width, height));
    return clamp((1.0 - d) / fwidth(d), 0.0, 1.0);
}
]]>
    </code>
</function>
